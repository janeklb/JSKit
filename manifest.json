{
	"name": "Chrome JSKit",
	"version": "0.2.3",
	"description": "Load common JavaScript libraries into any page",
	"permissions": ["tabs"],
	"background": {
		"scripts": ["vendors/jquery.min.js", "vendors/underscore-min.js", "vendors/backbone-min.js", "application.js", "background.js"]
	},
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"js": ["content_script.js"]
		}
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "JSKit",
		"default_popup": "popup.html"
	},
	"manifest_version": 2
}
